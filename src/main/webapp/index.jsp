<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>AdminLTE 3 | Log in (v2)</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="ExternalStylingAndLibraries/plugins/fontawesome-free/css/all.min.css">
    <!-- icheck bootstrap -->
    <link rel="stylesheet" href="ExternalStylingAndLibraries/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="ExternalStylingAndLibraries/dist/css/adminlte.min.css">
</head>
<body class="hold-transition login-page" style="background-color: #edf5fa;">

<div class="login-box w-50">
    <!-- /.login-logo -->
    <div class="card card-outline card-primary">
        <div class="card-header text-center mt-3 mb-3">
            <p class="h1">Light<b>NMS</b></p>
        </div>
        <div class="card-body">
            <p class="login-box-msg h3 mt-3 mb-3">Sign in</p>

            <form action="ExternalStylingAndLibraries/index3.html" method="post">
                <div class="input-group mb-4 w-50 mx-auto">
                    <input type="text" class="form-control" placeholder="Username">
                    <div class="input-group-append">
                        <div class="input-group-text">
                            <span class="fas fa-user"></span>
                        </div>
                    </div>
                </div>
                <div class="input-group mb-5 w-50 mx-auto">
                    <input type="password" class="form-control" placeholder="Password">
                    <div class="input-group-append">
                        <div class="input-group-text">
                            <span class="fas fa-lock"></span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <!-- /.col -->
                    <div class="col-8 mx-auto mb-5">
                        <button type="submit" class="btn btn-primary btn-block">Sign In</button>
                    </div>
                    <!-- /.col -->
                </div>
            </form>
        </div>
        <!-- /.card-body -->
    </div>
    <!-- /.card -->
</div>
<!-- /.login-box -->

<!-- jQuery -->
<script src="ExternalStylingAndLibraries/plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap 4 -->
<script src="ExternalStylingAndLibraries/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- AdminLTE App -->
<script src="ExternalStylingAndLibraries/dist/js/adminlte.min.js"></script>
</body>
</html>
